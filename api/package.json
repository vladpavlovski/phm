{
  "name": "hockey-management-system-api",
  "version": "0.0.1",
  "description": "API app for HMS",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env DEBUG=neo4j-graphql-js ./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/schema.graphql build",
    "start": "npm run build && node build/index.js",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js"
  },
  "author": "Vlad Pavlovski",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "apollo-server": "^2.19.1",
    "apollo-server-lambda": "^2.19.1",
    "csv-parse": "^4.14.2",
    "dotenv": "^8.2.0",
    "express-jwt": "^6.0.0",
    "neo4j-driver": "^4.2.1",
    "neo4j-graphql-js": "^2.18.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime-corejs3": "^7.12.5",
    "babel-plugin-auto-import": "^1.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "shx": "^0.3.3"
  }
}
