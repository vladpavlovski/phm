{
  "name": "hms-api",
  "version": "0.0.1",
  "description": "API app for HMS",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env DEBUG=@neo4j/graphql:* ./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  src/index.js",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/schema.graphql build",
    "start": "npm run build && node build/index.js",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js"
  },
  "author": "Vlad Pavlovski",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@neo4j/graphql": "^2.3.0",
    "apollo-server": "^3.4.0",
    "apollo-server-lambda": "^3.4.0",
    "aws-sdk": "^2.1009.0",
    "csv-parse": "^4.16.3",
    "dotenv": "^10.0.0",
    "express-jwt": "^6.1.0",
    "graphql": "^15.6.1",
    "neo4j-driver": "^4.3.4",
    "node-fetch": "^3.0.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.4",
    "babel-plugin-auto-import": "^1.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.13",
    "shx": "^0.3.3"
  }
}
